{
  "name": "trae-darkbulue",
  "displayName": "trae-darkbulue",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.1"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.20.55",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Trae Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark_plus.json"
      },
      {
        "label": "Trae Dark Blue",
        "uiTheme": "vs-dark",
        "path": "./themes/dark_blue.json"
      },
			{
        "label": "Trae Modern",
        "uiTheme": "vs-dark",
        "path": "./themes/dark_modern.json"
      },
      {
        "label": "Trae-darkblue",
        "uiTheme": "vs-dark",
        "path": "./themes/trae-darkblue-color-theme.json"
      },
      {
        "label": "Trae Light",
        "uiTheme": "vs",
        "path": "./themes/light_icube.json"
      },
      {
        "label": "Trae Light Modern",
        "uiTheme": "vs",
        "path": "./themes/light_modern.json"
      }
    ]
  }
}
